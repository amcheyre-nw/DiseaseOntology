<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/disease_ontology.owl"
         xmlns="http://example.org/disease_ontology.owl#">

<owl:Ontology rdf:about="http://example.org/disease_ontology.owl"/>

<owl:ObjectProperty rdf:about="#isSymptomOf">
  <rdfs:domain rdf:resource="#Disease"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isComplicationOf">
  <rdfs:domain rdf:resource="#Disease"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isCauseOf">
  <rdfs:domain rdf:resource="#Disease"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isTreatmentOf">
  <rdfs:domain rdf:resource="#Disease"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isPreventionOf">
  <rdfs:domain rdf:resource="#Disease"/>
</owl:ObjectProperty>

<owl:Class rdf:about="#Disease">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Complications">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Causes">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Prevention">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Treatment">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>


</rdf:RDF>
